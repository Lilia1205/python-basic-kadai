    "class Human:\n",
    "    def __init__(self, name, age):\n",
    "       \n",
    "        self.name = name  # 名前\n",
    "        self.age = age  # 年齢\n",
    "    def printinfo(self):#出力する内容\n",
    "        print(f\"名前:{self.name},年齢:{self.age}\")    \n",
    "\n",
    "person = Human(\"太郎\", 25)#具体的には\n",
    "person.printinfo()#Person?"
